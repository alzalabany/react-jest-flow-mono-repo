[ignore]
.*/node_modules/.*

[include]

[libs]
flow-typed

[lints]
all=warn
untyped-type-import=error
sketchy-null-bool=off

[options]
emoji=true
esproposal.export_star_as=enable
log.file=./flow.log
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=packages

[strict]
